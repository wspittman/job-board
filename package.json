{
  "name": "job-board",
  "version": "1.0.0",
  "scripts": {
    "start:backend": "npm run dev --workspace=backend",
    "start:frontend": "npm run dev --workspace=frontend",
    "start:frontend:cast": "npm run dev --workspace=frontend -- --host",
    "link-dry-utils": "npm link dry-utils-async dry-utils-text dry-utils-openai dry-utils-cosmosdb",
    "unlink-dry-utils": "npm unlink dry-utils-async dry-utils-text dry-utils-openai dry-utils-cosmosdb",
    "eval": "npm run eval --workspace=evals --",
    "eval-fetch-input": "npm run fetch-input --workspace=evals --",
    "admin": "npm run admin --workspace=admin",
    "clean": "git clean -fdx -- \":(glob)**/node_modules/**\" \":(glob)**/deploy/**\" \":(glob)**/dist/**\" \":(glob)**/*.tsbuildinfo\""
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ]
}
